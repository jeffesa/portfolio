{
  "name": "cielo_novo_blog",
  "version": "1.0.0",
  "description": "Novo layout de Cielo Blog",
  "scripts": {
    "lint": "eslint src/js/**",
    "dev": "concurrently \"gulp css:dev\" \"yarn lint && gulp js:dev\" \"gulp markup:dev\" \"gulp images:dev\"",
    "dist": "concurrently \"gulp css:dist\" \"yarn lint && gulp js:dist\" \"gulp markup:dist\" \"gulp images:dist\"",
    "css:dist": "concurrently \"gulp css:dist\"",
    "js:dist": "concurrently \"gulp js:dist\"",
    "start": "yarn dev && gulp watch",
    "js": "yarn lint && gulp js:dev",
    "js-watch": "yarn js && gulp watch:js",
    "css": "gulp css:dev",
    "css-watch": "gulp css:dev && gulp watch:css"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "autoprefixer": "^6.6.1",
    "babelify": "^10.0.0",
    "browser-sync": "^2.18.6",
    "browserify": "^16.5.0",
    "concurrently": "^3.5.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-hb": "^8.0.0",
    "gulp-postcss": "^6.2.0",
    "gulp-purgecss": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^2.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "flickity": "^2.2.1",
    "gulp-imagemin": "^6.1.1",
    "handlebars-layouts": "^3.1.4",
    "vanilla-masker": "^1.2.0"
  }
}
